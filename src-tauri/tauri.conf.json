{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:8000",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "软软AI",
    "version": "0.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http": {
        "scope": [
          "ws://**",
          "wss://**",
          "http://**",
          "https://**"
        ]
      },
      "shell": {
        "all": false,
        "sidecar": true,
        "open": true,
        "scope": [
          {
            "name": "binaries/ipfs",
            "sidecar": true,
            "args": [
              "daemon"
            ]
          }
        ]
      },
      "protocol": {
        "all": true
      },
      "window": {
        "all": true
      }
    },
    "macOSPrivateApi": true,
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "externalBin": [
        "binaries/ipfs"
      ],
      "identifier": "link.idns.rrai.desktop",
      "targets": "all",
      "windows": {
        "wix": {
          "language": "zh-CN"
        }
      }
    },
    "security": {
      "dangerousRemoteDomainIpcAccess": [
        {
          "scheme": "rrapp",
          "enableTauriAPI": true,
          "domain": "localhost",
          "plugins": [
            "rrai-sqlite",
            "rrai-storage"
          ],
          "windows": [
            "main",
            "rrapp"
          ]
        }
      ],
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/rrkeji/rrai-desktop/releases/download/updater/update.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDNBQjFEMEIyRTkzQTQzNjIKUldSaVF6cnBzdEN4T3BJZUR2RHVrUnFzcG9BTzEyOTM3VWlSQ3grRmI3OHFSQlBLK1NBaWdKbWcK",
      "windows": {
        "installMode": "passive"
      }
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "title": "软软AI",
        "width": 1330,
        "minWidth": 900,
        "minHeight": 600,
        "height": 1020,
        "fileDropEnabled": true,
        "decorations": false,
        "transparent": true,
        "visible": true
      },
      {
        "label": "rrapp",
        "url": "rrapp://localhost/?cid=QmQ4BmfU9NA467PYySrVgrtYeftf255uAnXqTRWhTpcaHM",
        "fullscreen": false,
        "resizable": true,
        "title": "软软AI",
        "width": 1330,
        "minWidth": 900,
        "minHeight": 600,
        "height": 1020,
        "fileDropEnabled": true,
        "decorations": false,
        "transparent": true,
        "visible": true
      }
    ]
  }
}