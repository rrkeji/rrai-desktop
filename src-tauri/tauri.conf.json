{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:8000",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "rrai-desktop",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http": {
        "scope": [
          "http://**",
          "https://**"
        ]
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "link.idns.rrai.desktop",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "软软AI",
        "width": 1024,
        "height": 768,
        "fileDropEnabled": true,
        "decorations": true,
        "visible": true
      }
    ]
  }
}